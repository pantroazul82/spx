                    <CommandText>SELECT 
usuarios.nombre 'responsable',
repuestos.codRepuesto,max(repuestos.descripcion) 'descripcion',
max(repuestos.precio) 'Precio',sum(repuestosxOrdenPrestamo.cantidadSolicitada) 'cantidadSolicitada',
sum(repuestosxOrdenPrestamo.cantidadPrestada) 'cantidadPrestada',
sum(repuestosxOrdenPrestamo.cantidadDevuelta) 'cantidadDevuelta',
sum(repuestosxOrdenPrestamo.cantidadConNotaSalida) 'cantidadConNotaSalida'
 
FROM repuestosxOrdenPrestamo
join ordenPrestamo on ordenPrestamo.codOrdenPrestamo = repuestosxOrdenPrestamo.codOrdenPrestamo
join repuestos on repuestos.codRepuesto = repuestosxOrdenPrestamo.codRepuesto
join usuario usuarios  on usuarios.login =ordenPrestamo.loginResponsable

where 
ordenPrestamo.codEstadoOrdenPrestamo != 8

group by
usuarios.nombre,
repuestos.codRepuesto
having (
sum(repuestosxOrdenPrestamo.cantidadPrestada)&gt;
sum(repuestosxOrdenPrestamo.cantidadDevuelta) +
sum(repuestosxOrdenPrestamo.cantidadConNotaSalida) 
)
order by responsable</CommandText>
