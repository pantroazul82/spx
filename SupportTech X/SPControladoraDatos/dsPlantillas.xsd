              <DbSource ConnectionRef="SUPPORTECHConnectionString (Settings)" DbObjectName="SELECT CASE WHEN datediff(day,getdate(),fechaProgramacion) =0 THEN 'Para Hoy' WHEN datediff(day,getdate(),fechaProgramacion) =1 THEN 'Para Mañana' WHEN datediff(day,getdate(),fechaProgramacion) &lt;0 AND datediff(day,getdate(),fechaProgramacion)&gt;-7 THEN 'Vencidos Menos de Una Semana' WHEN datediff(day,getdate(),fechaProgramacion) &lt; (-7) THEN 'Vencidos Mas de Una Semana' END 'vencido' ,fechaProgramacion,codOrdenServicio FROM ordenServicio WHERE fechaProgramacion BETWEEN @ini AND @fin" DbObjectType="Unknown" FillMethodModifier="Public" FillMethodName="FillByVencimiento" GenerateMethods="Both" GenerateShortCommands="true" GeneratorGetMethodName="GetDataByVencimiento" GeneratorSourceName="FillByVencimiento" GetMethodModifier="Public" GetMethodName="GetDataByVencimiento" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="true" UserGetMethodName="GetDataByVencimiento" UserSourceName="FillByVencimiento">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>
select vencido 'Nombre',count(codOrdenServicio) 'cantidad'
from(
select  
case  when datediff(day,getdate(),fechaProgramacion) =0 then 'Para Hoy'
      when datediff(day,getdate(),fechaProgramacion) =1 then 'Para Mañana'
when datediff(day,getdate(),fechaProgramacion) &lt;0 and datediff(day,getdate(),fechaProgramacion)&gt;-7
then 'Vencidos Menos de Una Semana'
when datediff(day,getdate(),fechaProgramacion) &lt; (-7)
then 'Vencidos Mas de Una Semana'
end 'vencido'
,fechaProgramacion,codOrdenServicio
 from ordenServicio
where fechaProgramacion between @ini and @fin
) f
group by vencido


</CommandText>
