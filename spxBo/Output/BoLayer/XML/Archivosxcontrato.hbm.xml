 >
<key-property 
name="NumeroContrato"
column="`numeroContrato`"
type="System.String"
length="40"
>
</key-property>
<key-property 
name="CodOtroSi"
column="`codOtroSi`"
type="System.Int32"
>
</key-property>
<key-property 
name="CodArchivo"
column="`codArchivo`"
type="System.Int32"
>
</key-property>
</composite-id> 
<property
name="NombreArchivo"
column="`nombreArchivo`"
type="System.String"
length="50"
    >
</property>
<property
name="Descripcion"
column="`descripcion`"
type="System.String"
length="250"
    >
</property>
<many-to-one
