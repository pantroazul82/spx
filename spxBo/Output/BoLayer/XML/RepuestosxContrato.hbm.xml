<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" 
 namespace="BOLayer" 
 assembly="BOLayer" >
<class name="RepuestosxContrato" 
 table="[dbo].[repuestosxContratos]" 
>
<composite-id unsaved-value="any"
 >
<key-property 
name="NumeroContrato"
column="`numeroContrato`"
type="System.String"
length="40"
>
</key-property>
<key-property 
name="CodOtroSi"
column="`codOtroSi`"
type="System.Int32"
>
</key-property>
<key-property 
name="CodRepuesto"
column="`codRepuesto`"
type="System.String"
length="20"
>
</key-property>
</composite-id> 
<property
name="Cantidad"
column="`cantidad`"
type="System.Nullable`1[System.Int32]"
    >
</property>
<property
name="Observaciones"
column="`observaciones`"
type="System.String"
length="250"
    >
</property>
<many-to-one
 name="Contrato" 
 class="Contrato" 
 cascade="none" 
 outer-join="auto" 
 insert="false" 
 update="false" 
>
<column name="`numeroContrato`" />
<column name="`codOtroSi`" />
</many-to-one>
<many-to-one
 name="Repuesto" 
 class="Repuesto" 
 cascade="none" 
 outer-join="auto" 
 insert="false" 
 update="false" 
>
<column name="`codRepuesto`" />
</many-to-one>
</class>
</hibernate-mapping>

